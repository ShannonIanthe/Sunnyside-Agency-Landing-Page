{
    "name": "project",
    "version": "0.1.0",
    "description": "SASS compile|autoprefix|minimize and live-reload dev server using Browsersync for static HTML",
    "main": "public/index.html",
    "author": "5t3ph",
    "scripts": {
        "sass": "node-sass -w scss/ -o dist/css/ --recursive",         "scss": "node-sass --watch src -o src",
        "watch-sass": "node-sass scss/style.scss css/main.css --watch",
        "compile:sass":"node-sass scss/style.scss css/main.css -w"
    },
    "dependencies": {
        "browser-sync": "^2.27.7",
        "copyfiles": "^2.4.1",
        "cssnano": "^5.0.17",
        "font-awesome": "^4.7.0",
        "font-awesome-icons": "^1.6.0",
        "onchange": "^7.1.0",
        "sass": "^1.49.8",
        "webpack-cli": "^5.0.1",
        "node-sass": "^4.12.0"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.13",
        "concat": "^1.0.3",
        "npm-run-all": "^4.1.5",
        "parcel": "^2.8.2",
        "postcss-cli": "^10.1.0"
    }
}
